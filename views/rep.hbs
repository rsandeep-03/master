<script>
  function scoredata() {
    var multip_one = 1;
    var multip_two = 2;
    var mulip_five = 5;

    var item1 = document.getElementById('result1').value;
    var item2 = document.getElementById('result2').value;
    var item3 = document.getElementById('result3').value;
    var item4 = document.getElementById('result4').value;
    var item5 = document.getElementById('result5').value;
    var item6 = document.getElementById('result6').value;
    var item7 = document.getElementById('result7').value;
    var item8 = document.getElementById('result8').value;

    if (item1 === "") {
      item1 = 0;
    }
    if (item2 === "") {
      item2 = 0;
    }
    if (item3 === "") {
      item3 = 0;
    }
    if (item4 === "") {
      item4 = 0;
    }
    if (item5 === "") {
      item5 = 0;
    }
    if (item6 === "") {
      item6 = 0;
    }
    if (item7 === "") {
      item7 = 0;
    }
    if (item8 === "") {
      item8 = 0;
    }

    var score1 = parseInt(multip_two) * parseFloat(item1);
    var score2 = parseInt(multip_two) * parseFloat(item2);
    var score3 = parseInt(multip_five) * parseFloat(item3);
    var score4 = parseInt(multip_two) * parseFloat(item4);
    var score5 = parseInt(multip_one) * parseFloat(item5);
    var score6 = parseInt(multip_one) * parseFloat(item6);
    var score7 = parseInt(multip_two) * parseFloat(item7);
    var score8 = parseInt(multip_one) * parseFloat(item8);

    document.getElementById('scoretot1').innerHTML = score1.toFixed(2);
    document.getElementById('scoretot2').innerHTML = score2.toFixed(2);
    document.getElementById('scoretot3').innerHTML = score3.toFixed(2);
    document.getElementById('scoretot4').innerHTML = score4.toFixed(2);
    document.getElementById('scoretot5').innerHTML = score5.toFixed(2);
    document.getElementById('scoretot6').innerHTML = score6.toFixed(2);
    document.getElementById('scoretot7').innerHTML = score7.toFixed(2);
    document.getElementById('scoretot8').innerHTML = score8.toFixed(2);
    document.getElementById('score_1').value = score1.toFixed(2);
    document.getElementById('score_2').value = score2.toFixed(2);
    document.getElementById('score_3').value = score3.tofixed(2);
    document.getElementById('score_4').value = score4.toFixed(2);
    document.getElementById('score_5').value = score5.toFixed(2);
    document.getElementById('score_6').value = score6.toFixed(2);
    document.getElementById('score_7').value = score7.toFixed(2);
    document.getElementById('score_8').value = score8.toFixed(2);
    var Total = parseFloat(score1) + parseFloat(score2) + parseFloat(score3) + parseFloat(score4) + parseFloat(score5) + parseFloat(score6) + parseFloat(score7) + parseFloat(score8);
    document.getElementById('totalscore').innerHTML = Total.toFixed(2);
    document.getElementById('total_value').value = Total.toFixed(2);

    var Level = (parseFloat(Total) - 80) / 5;

    document.getElementById('level').innerHTML = Level.toFixed(2);
    document.getElementById('total_level').value = Level.toFixed(2);
}
</script>
